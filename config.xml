    <gap:splash gap:platform="ios" src="resources/ios/splash/Default~iphone.png" width="320" height="480"/>	
	
	

	
	
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <allow-intent href="http://*/*" />

    <engine name="android" spec="^4.0.0" />
</widget>
